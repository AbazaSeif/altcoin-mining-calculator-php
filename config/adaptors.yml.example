# Config settings for a selection of RPC based adaptors
Hirocoin:
    type: \n00bsys0p\RpcAdaptor
    subsidy_function: \n00bsys0p\HirocoinSubsidyFunction
    host: 127.0.0.1
    port: 9347
    user: hirocoinrpc
    pass: Y0urSup3rSecretP4s5word
Darkcoin:
    type: \n00bsys0p\RpcAdaptor
    subsidy_function: \n00bsys0p\DarkcoinSubsidyFunction
    host: 127.0.0.1
    port: 9998
    user: darkcoinrpc
    pass: Y0urSup3rSecretP4s5word
Limecoin:
    type: \n00bsys0p\RpcAdaptor
    subsidy_function: \n00bsys0p\LimecoinSubsidyFunction
    host: 127.0.0.1
    port: 9031
    user: limecoinrpc
    pass: Y0urSup3rSecretP4s5word
Québecoin:
    type: \n00bsys0p\RpcAdaptor
    subsidy_function: \n00bsys0p\LimecoinSubsidyFunction
    host: 127.0.0.1
    port: 56789
    user: québecoinrpc # NOTE: my system didn't like the default, so I changed it to quebecoinrpc here and in quebecoin.conf
    pass: Y0urSup3rSecretP4s5word
